{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/((?!api/|src/|assets/).*)", "destination": "/index.html" }
  ]
}
